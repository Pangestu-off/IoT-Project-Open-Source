  display.println(F("the Resistor value is"));
  display.setCursor(20,25);
  display.setTextSize(2);             
  display.setTextColor(WHITE);        
  display.print(Ru);
  display.print(F(" Ohm"));
  display.drawBitmap(0,0, myBitmap, 128, 64, WHITE);
  display.display();
  delay(2000);
}
